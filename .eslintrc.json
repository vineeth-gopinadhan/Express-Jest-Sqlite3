{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "require-jsdoc" : "off",
        "object-curly-spacing": ["error", "always"],
        "quotes": ["error"],
        "comma-dangle": ["error", "never"],
        "new-cap": "off"
    }
}
